<header class="p-3 text-bg-dark">
  <div class="container">
    <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
      <a href="#" class="d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none">
        <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap"></use></svg>
      </a>

      <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
        <li><a href="/" class="nav-link px-2 text-secondary">Home</a></li>
        <li><a href="/properties" class="nav-link px-2 text-white">Properties</a></li>
        <li><a href="#" class="nav-link px-2 text-white">Check in</a></li>
        <li><a href="#" class="nav-link px-2 text-white">Check out</a></li>
        <li><a href="#" class="nav-link px-2 text-white">People</a></li>
      </ul>


      <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3" role="search">
        <input type="search" class="form-control form-control-dark text-bg-dark" placeholder="Search..." aria-label="Search">
      </form>
      <% if account_signed_in? %>
        <div class="dropdown text-end">
          <a href="#" class="d-block link-dark text-decoration-none dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
            <img src="https://upload.wikimedia.org/wikipedia/commons/9/97/IU_at_Broker_GV_preview%2C_8_June_2022.jpg" width="60" height="60" class="rounded-circle">
          </a>
          <ul class="dropdown-menu dropdown-menu-dark" style="">
            <li><a class="dropdown-item rounded-2 active" href="#">Bookings</a></li>
            <li><a class="dropdown-item rounded-2" href="#">Documents</a></li>
            <li><a class="dropdown-item rounded-2" href="#">Account</a></li>
<!--            <li><hr class="dropdown-divider"></li>-->
            <li><a class="dropdown-item rounded-2" href="#">Help</a></li>
            <li><a class="dropdown-item rounded-2" href="#">Wishlists</a></li>
            <%= button_to "Log out", destroy_account_session_path, method: :delete, class: "dropdown-item rounded-2" %></button>
          </ul>
<!--            <%#= button_to "Log out", destroy_account_session_path, method: :delete, class: "btn btn-outline-light me-2" %></button>-->
<!--          <button type="button" class="btn btn-outline-light me-2"><%#= link_to "Log out", destroy_account_session_path, method: :delete, class: "nav-link" %></button>-->
        </div>
      <% else %>
        <div class="text-end">
          <a href="#" class="d-block link-dark text-decoration-none dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
            <img src="https://cdn-icons-png.flaticon.com/512/25/25634.png" width="30" height="30" class="rounded-circle">
          </a>
          <ul class="dropdown-menu dropdown-menu-dark" style="">
            <button type="button" class="dropdown-item rounded-2" data-bs-toggle="modal" data-bs-target="#modalsignup" data-bs-whatever="@mdo">Sign Up</button>
<!--            <%#= button_to "Sign Up", new_account_registration_path, method: :get, class: "dropdown-item rounded-2" %></button>-->
            <button type="button" class="dropdown-item rounded-2" data-bs-toggle="modal" data-bs-target="#modalsignin" data-bs-whatever="@mdo">Log In</button>
<!--            <%#= button_to "Log In", new_account_session_path, method: :get, class: "dropdown-item rounded-2" %></button>-->
            <li><hr class="dropdown-divider"></li>
            <li><a class="dropdown-item rounded-2" href="#">Host Home</a></li>
            <li><a class="dropdown-item rounded-2" href="#">Help</a></li>
          </ul>
<!--          <button type="button" class="btn btn-outline-light me-2"><%#= link_to "Log in", new_account_session_path, class: "nav-link" %></button>-->
<!--          <button type="button" class="btn btn-warning"><%#= link_to "Sign up", new_account_registration_path, class: "nav-link" %></button>-->
        </div>
      <% end %>
    </div>
  </div>
</header>

