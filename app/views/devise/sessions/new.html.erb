<section class="form-auth">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<!--  <link href="devise.css" rel="stylesheet">-->
  <body class="text-center">

  <main class="form-signin w-100 m-auto">
<!--      <img class="mb-4" src="/docs/5.2/assets/brand/bootstrap-logo.svg" alt="" width="72" height="57">-->
      <h1 class="h3 mb-3 fw-normal">Please sign in</h1>

      <div class="field">
<!--        <input type="email" class="form-control" id="floatingInput" placeholder="name@example.com">-->

        <%= f.email_field :email, autofocus: true,class: "form-control", id: "inputEmail", placeholder: "Email address", autocomplete: "email" %>
        <%#= f.email_field :email, autofocus: true, autocomplete: "email" %>

      </div>
      <div class="field">
<!--        <input type="password" class="form-control" id="floatingPassword" placeholder="Password">-->
        <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class: "form-control", id: "inputPassword" %>
        <%#= f.password_field :password, autocomplete: "current-password" %>
      </div>

      <div class="checkbox mb-3">
        <label>
          <input type="checkbox" value="remember-me"> Remember me
        </label>
      </div>
      <button class="w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
      <p class="mt-5 mb-3 text-muted">© 2017–2022</p>
  <% end %>
  </main>

  </body>
  <%= render "devise/shared/links" %>
</section>



